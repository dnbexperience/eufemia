{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ],
  "ignore": [
    ".*node_modules/(?!rewrite-pattern|regexpu-core|buble|react-live).*"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"]
}
