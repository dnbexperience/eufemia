{
  "private": true,
  "name": "dnb-design-system-portal",
  "version": "1.0.0",
  "description": "DNB Design System Portal",
  "license": "SEE LICENSE IN LICENSE FILE",
  "author": "DNB Team & Tobias HÃ¸egh",
  "main": "src/docs/index.js",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=16384 gatsby build",
    "build:ci": "yarn build:version && gatsby build --prefix-paths",
    "build:mini": "BUILD_MINI=1 gatsby build --prefix-paths",
    "build:version": "node ./scripts/version.js --new-version",
    "build:visual-test": "SKIP_IMAGE_PROCESSING=1 IS_VISUAL_TEST=1 NODE_OPTIONS=--max-old-space-size=16384 gatsby build --no-uglify",
    "clean": "gatsby clean",
    "lint": "eslint --quiet .",
    "lint:ci": "NODE_ENV=test yarn lint:js && yarn lint:styles && yarn prettier:check && yarn prettier:package",
    "lint:js": "yarn lint",
    "lint:styles": "stylelint './src/**/*.scss'",
    "prettier:check": "prettier --check '**/*.{md,mdx,js,ts,tsx}'",
    "prettier:package": "prettier-package-json --list-different ./package.json",
    "prettier:package:write": "prettier-package-json --write ./package.json",
    "prettier:write": "prettier --log-level warn --write '**/*.{md,mdx,js,ts,tsx}'",
    "reset": "yarn clean && rm -rf ./node_modules",
    "serve": "gatsby serve -p 8000",
    "serve:8001": "gatsby serve -p 8001",
    "serve:8002": "gatsby serve -p 8002",
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=16384 gatsby develop",
    "test": "jest",
    "test:ci": "jest --ci --passWithNoTests",
    "test:e2e:portal": "yarn playwright test",
    "test:e2e:portal:ci": "start-server-and-test serve:8002 http://localhost:8002 test:e2e:portal",
    "test:e2e:portal:watch": "playwright test --ui",
    "test:screenshots": "yarn workspace @dnb/eufemia test:screenshots",
    "test:screenshots:ci": "start-server-and-test serve http://localhost:8000 'yarn workspace @dnb/eufemia test:screenshots:ci'",
    "test:screenshots:ci:conditional": "start-server-and-test serve http://localhost:8000 'yarn workspace @dnb/eufemia test:screenshots:ci:conditional'",
    "test:screenshots:ci:update": "start-server-and-test serve http://localhost:8000 'yarn workspace @dnb/eufemia test:screenshots:ci:update'",
    "test:types": "tsc --noEmit",
    "test:types:watch": "tsc --noEmit --watch",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@dnb/eufemia": "workspace:*",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mdx-js/mdx": "3.1.0",
    "@mdx-js/react": "3.1.0",
    "algoliasearch": "4.12.0",
    "classnames": "2.3.1",
    "history": "5.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/eslint-parser": "7.28.5",
    "@dnb/browserslist-config": "1.2.0",
    "@emotion/cache": "11.11.0",
    "@playwright/test": "1.49.1",
    "@types/json-schema": "7.0.12",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/parser": "8.52.0",
    "babel-jest": "30.2.0",
    "cross-env": "7.0.3",
    "current-git-branch": "1.1.0",
    "date-fns": "4.1.0",
    "dotenv": "10.0.0",
    "eslint": "9.39.2",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-workspaces": "0.11.1",
    "eufemia-llm-metadata": "workspace:*",
    "front-matter": "4.0.2",
    "fs-extra": "10.0.0",
    "gatsby": "5.14.3",
    "gatsby-plugin-algolia": "1.0.3",
    "gatsby-plugin-babel-react-live": "1.4.5",
    "gatsby-plugin-catch-links": "5.14.0",
    "gatsby-plugin-emotion": "8.14.0",
    "gatsby-plugin-eufemia-theme-handler": "1.8.0",
    "gatsby-plugin-manifest": "5.14.0",
    "gatsby-plugin-mdx": "5.14.1",
    "gatsby-plugin-meta-redirect": "1.1.1",
    "gatsby-plugin-offline": "6.14.0",
    "gatsby-plugin-page-creator": "5.14.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sass": "6.14.0",
    "gatsby-plugin-scroll-position": "1.3.3",
    "gatsby-plugin-sharp": "5.14.0",
    "gatsby-remark-images": "7.14.0",
    "gatsby-source-filesystem": "5.14.0",
    "github-slugger": "1.4.0",
    "jest": "30.0.5",
    "mdast-util-to-string": "^2",
    "postcss": "8.4.32",
    "prettier": "3.0.3",
    "prettier-package-json": "2.8.0",
    "prism-react-renderer": "2.0.5",
    "process": "0.11.10",
    "prop-types": "15.8.1",
    "raw-loader": "4.0.2",
    "react-live-ssr": "workspace:*",
    "react-markdown": "8.0.7",
    "remark-gfm": "1.0.0",
    "repo-utils": "workspace:*",
    "sass": "1.62.0",
    "start-server-and-test": "2.0.1",
    "storybook-utils": "workspace:*",
    "stylelint": "15.9.0",
    "stylelint-config-recommended": "12.0.0",
    "stylelint-config-standard-scss": "9.0.0",
    "typescript": "5.9.3",
    "unist-util-visit": "^2"
  },
  "buildVersion": "[LOCAL BUILD]",
  "changelogVersion": "[LOCAL BUILD]",
  "releaseVersion": "[LOCAL BUILD]",
  "volta": {
    "extends": "../../package.json"
  }
}
