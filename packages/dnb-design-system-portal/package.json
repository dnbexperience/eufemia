{
  "private": true,
  "name": "dnb-design-system-portal",
  "description": "DNB Design System Portal",
  "license": "SEE LICENSE IN LICENSE FILE",
  "author": "DNB Team & Tobias HÃ¸egh",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "gatsby build",
    "build:legacy": "cross-env BUILD_LEGACY_BROWSERS=1 gatsby build",
    "build:version": "babel-node ./scripts/version.js --new-version",
    "build-ci": "yarn build:version && yarn build --prefix-paths && yarn commit-new-pages",
    "clean": "gatsby clean",
    "precommit": "yarn lint-staged",
    "commit-new-pages": "babel-node ./scripts/commitPages.js",
    "cy": "cypress open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --port 4000 --headless --spec './cypress/integration/**/*'",
    "cypress:run-ci": "start-server-and-test cypress:server http://localhost:3030 cypress:run",
    "cypress:server": "gatsby serve -p 3030",
    "deploy": "yarn deploy-ci",
    "deploy-ci": "babel-node ./scripts/deploy.js",
    "deploy:fast": "yarn build --prefix-paths && babel-node ./scripts/deploy.js",
    "dev": "nodemon --exec 'gatsby develop' --watch 'gatsby-node.js' --watch 'gatsby-config.js'",
    "lint": "eslint --quiet ./src",
    "lint:ci": "yarn lint:js && yarn lint:styles",
    "lint-staged": "lint-staged",
    "lint:js": "yarn lint --fix",
    "lint:js:staged": "eslint --quiet --fix",
    "lint:styles": "stylelint './src/**/*.{js,css}'",
    "lint:styles:staged": "stylelint './src/**/*.{js,css}'",
    "reset": "rm -rf ./node_modules && yarn clean",
    "serve": "gatsby serve -p 8000",
    "start": "cross-env gatsby develop",
    "test": "jest --watch",
    "test:ci": "jest --ci --passWithNoTests",
    "test:staged": "jest --bail --findRelatedTests ",
    "test:update": "jest --updateSnapshot",
    "prettier:components": "prettier --loglevel warn --write './src/uilib/{components,extensions}/**/*.{js,md}' './src/docs/uilib/{components,elements,extensions}/*.{js,md}'",
    "prettier:diff": "prettier --list-different '{scripts,stories,src}/**/*.{js,md}'",
    "prettier:js": "prettier --loglevel warn --write '{scripts,stories,src}/**/*.js'",
    "prettier:js:staged": "prettier --loglevel warn",
    "prettier:other": "prettier --loglevel warn --write 'src/**/*.{json,md}' --write '.babelrc' && yarn prettier:package",
    "prettier:other:staged": "prettier --loglevel warn",
    "prettier:package": "prettier-package-json --write ./package.json"
  },
  "dependencies": {
    "algoliasearch": "^4.10.5",
    "css-vars-ponyfill": "^2.4.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/node": "^7.15.4",
    "@dnb/eufemia": "workspace:*",
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@netlify/plugin-gatsby": "^1.0.3",
    "babel-jest": "^27.2.1",
    "babel-preset-gatsby": "^1.14.0",
    "camelcase": "^6.2.0",
    "ci-info": "^3.2.0",
    "classnames": "^2.3.1",
    "clean-html": "^1.5.0",
    "cross-env": "^7.0.3",
    "current-git-branch": "^1.1.0",
    "date-fns": "^2.24.0",
    "del": "^6.0.0",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.0.0",
    "gatsby": "^3.14.0",
    "gatsby-link": "^3.14.0",
    "gatsby-plugin-algolia": "^0.22.2",
    "gatsby-plugin-catch-links": "^3.14.0",
    "gatsby-plugin-emotion": "^6.14.0",
    "gatsby-plugin-gatsby-cloud": "^3.2.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-mdx": "^2.14.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-page-creator": "^3.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-react-router-scroll": "^4.14.0",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gh-pages": "^3.2.3",
    "github-slugger": "^1.4.0",
    "is-ci": "^3.0.0",
    "jest": "^27.2.1",
    "jest-emotion": "^11.0.0",
    "jest-raw-loader": "^1.0.1",
    "keycode": "^2.2.0",
    "lint-staged": "^11.1.2",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.12",
    "ora": "^5.4.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.3.7",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "prettier-package-json": "^2.6.0",
    "prism-react-renderer": "^1.2.1",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react-fake-props": "^1.0.3",
    "react-live": "^2.4.0",
    "react-markdown": "^7.0.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "svg-react-loader": "0.4.6",
    "yarn-plugin-ignore-install-options": "^1.0.2"
  },
  "installConfig": {
    "hoistingLimits": "dependencies"
  },
  "buildVersion": "[LOCAL BUILD]",
  "changelogVersion": "April, 5. 2021",
  "volta": {
    "extends": "../../package.json"
  }
