{
  "private": true,
  "name": "dnb-design-system-portal",
  "version": "1.0.0",
  "description": "DNB Design System Portal",
  "license": "SEE LICENSE IN LICENSE FILE",
  "author": "DNB Team & Tobias HÃ¸egh",
  "main": "index.js",
  "scripts": {
    "build": "NODE_NO_WARNINGS=1 gatsby build --no-uglify",
    "build-ci": "yarn build:version && yarn build --prefix-paths && yarn commit-new-pages",
    "build:version": "node ./scripts/version.js --new-version",
    "clean": "NODE_NO_WARNINGS=1 gatsby clean",
    "precommit": "yarn lint-staged",
    "commit-new-pages": "node ./scripts/commitPages.js",
    "cy": "cypress open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --port 4000 --headless --spec './cypress/integration/**/*'",
    "cypress:run-ci": "start-server-and-test cypress:server http://localhost:3030 cypress:run",
    "cypress:server": "gatsby serve -p 3030",
    "deploy": "yarn deploy-ci",
    "deploy-ci": "node ./scripts/deploy.js",
    "lint": "eslint --quiet ./src",
    "lint-staged": "lint-staged",
    "lint:ci": "yarn lint:js && yarn lint:styles",
    "lint:js": "yarn lint --fix",
    "lint:js:staged": "eslint --quiet --fix",
    "lint:styles": "stylelint './src/**/*.{js,css}'",
    "lint:styles:staged": "stylelint './src/**/*.{js,css}'",
    "reset": "rm -rf ./node_modules && yarn clean",
    "serve": "NODE_NO_WARNINGS=1 gatsby serve -p 8000",
    "start": "NODE_NO_WARNINGS=1 cross-env gatsby develop",
    "test": "jest",
    "test:ci": "jest --ci --passWithNoTests",
    "test:staged": "jest --bail --findRelatedTests ",
    "test:types": "tsc --noEmit",
    "test:types:watch": "tsc --noEmit --watch",
    "test:update": "jest --updateSnapshot",
    "prettier:components": "prettier --loglevel warn --write './src/uilib/{components,extensions}/**/*.{js,md}' './src/docs/uilib/{components,elements,extensions}/*.{js,md}'",
    "prettier:diff": "prettier --list-different '{scripts,stories,src}/**/*.{js,md}'",
    "prettier:js": "prettier --loglevel warn --write '{scripts,stories,src}/**/*.js'",
    "prettier:js:staged": "prettier --loglevel warn",
    "prettier:other": "prettier --loglevel warn --write 'src/**/*.{json,md}' --write '.babelrc' && yarn prettier:package",
    "prettier:other:staged": "prettier --loglevel warn",
    "prettier:package": "prettier-package-json --write ./package.json"
  },
  "dependencies": {
    "@dnb/eufemia": "workspace:*",
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "algoliasearch": "4.12.0",
    "classnames": "2.3.1",
    "css-vars-ponyfill": "2.4.7",
    "dnb-eufemia-sandbox": "workspace:*",
    "history": "5.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-router-dom": "5.3.0",
    "smoothscroll-polyfill": "0.4.4"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.0",
    "@babel/node": "7.16.0",
    "@emotion/cache": "11.5.0",
    "babel-preset-gatsby": "2.1.0",
    "camelcase": "6.2.0",
    "cross-env": "7.0.3",
    "current-git-branch": "1.1.0",
    "date-fns": "2.25.0",
    "del": "6.0.0",
    "dotenv": "10.0.0",
    "eslint": "8.1.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "next",
    "front-matter": "4.0.2",
    "fs-extra": "10.0.0",
    "gatsby": "4.1.2",
    "gatsby-link": "4.1.0",
    "gatsby-plugin-algolia": "0.25.0",
    "gatsby-plugin-catch-links": "4.1.0",
    "gatsby-plugin-emotion": "7.1.0",
    "gatsby-plugin-gatsby-cloud": "4.1.0",
    "gatsby-plugin-manifest": "4.1.1",
    "gatsby-plugin-mdx": "3.1.1",
    "gatsby-plugin-meta-redirect": "1.1.1",
    "gatsby-plugin-offline": "5.1.1",
    "gatsby-plugin-page-creator": "4.1.1",
    "gatsby-plugin-react-helmet": "5.1.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sass": "5.1.1",
    "gatsby-plugin-sharp": "4.1.1",
    "gatsby-react-router-scroll": "5.1.0",
    "gatsby-remark-images": "6.1.1",
    "gatsby-source-filesystem": "4.1.0",
    "gh-pages": "3.2.3",
    "github-slugger": "1.4.0",
    "is-absolute-url": "4.0.1",
    "jest": "27.3.1",
    "jest-emotion": "11.0.0",
    "keycode": "2.2.0",
    "lint-staged": "11.2.6",
    "nodemon": "2.0.15",
    "ora": "5.4.1",
    "postcss": "8.3.11",
    "postcss-preset-env": "6.7.0",
    "prettier-package-json": "2.6.0",
    "prism-react-renderer": "1.2.1",
    "process": "0.11.10",
    "prop-types": "15.7.2",
    "raw-loader": "4.0.2",
    "react-live": "2.4.0",
    "react-markdown": "7.1.0",
    "sass": "1.43.4",
    "start-server-and-test": "1.14.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "svg-react-loader": "0.4.6",
    "typescript": "4.5.5"
  },
  "buildVersion": "[LOCAL BUILD]",
  "releaseVersion": "[LOCAL BUILD]",
  "changelogVersion": "[LOCAL BUILD]",
  "installConfig": {
    "hoistingLimits": "dependencies"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
