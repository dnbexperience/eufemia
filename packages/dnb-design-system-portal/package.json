{
  "private": true,
  "name": "dnb-design-system-portal",
  "description": "DNB Design System Portal",
  "license": "SEE LICENSE IN LICENSE FILE",
  "author": "DNB Team & Tobias HÃ¸egh",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "yarn build-portal:version && yarn build-portal",
    "build-portal": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build-portal-ci": "yarn build-portal:version && yarn make-ui-lib-pages && yarn build-portal --prefix-paths && yarn commit-new-pages",
    "build-portal:version": "babel-node ./scripts/version.js --new-version",
    "build:dev": "yarn make-ui-lib-pages:dev",
    "clean": "rm -rf .cache ./public/**",
    "precommit": "yarn lint-staged",
    "commit-new-pages": "babel-node ./scripts/commitPages.js",
    "cy": "cypress open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --port 4000 --headless --spec './cypress/integration/**/*'",
    "cypress:run-ci": "start-server-and-test cypress:server http://localhost:3030 cypress:run",
    "cypress:server": "gatsby serve -p 3030",
    "deploy": "yarn deploy-ci",
    "deploy-ci": "babel-node ./scripts/deploy.js",
    "deploy:fast": "yarn build-portal --prefix-paths && babel-node ./scripts/deploy.js",
    "dev": "nodemon --exec 'gatsby develop' --watch 'gatsby-node.js' --watch 'gatsby-config.js'",
    "lint": "eslint --quiet ./src",
    "lint-ci": "yarn lint:js && yarn lint:styles",
    "lint-staged": "lint-staged",
    "lint:js": "yarn lint --fix",
    "lint:js:staged": "eslint --quiet --fix",
    "lint:styles": "stylelint './src/**/*.{js,css}'",
    "lint:styles:staged": "stylelint './src/**/*.{js,css}'",
    "make-ui-lib-pages": "babel-node ./scripts/uilib/makeDemosFactory.js && yarn prettier:components",
    "make-ui-lib-pages:dev": "nodemon --exec 'yarn make-ui-lib-pages' --ext js --watch '../dnb-ui-lib/src/**/*' --watch '**/makeDemosFactory.js' --watch '**/tasks/*' --watch '**/Template.js'  --watch '**/template.md'",
    "reset": "rm -rf ./node_modules && yarn clean",
    "serve": "gatsby serve -p 8000",
    "start": "cross-env DEBUG=gatsby:query-watcher GATSBY_HOT_LOADER=fast-refresh GATSBY_EXPERIMENTAL_QUERY_ON_DEMAND=1 GATSBY_EXPERIMENTAL_FAST_DEV=true gatsby develop",
    "test": "jest --watch",
    "test-ci": "jest --ci",
    "test:staged": "jest --bail --findRelatedTests ",
    "test:update": "jest --updateSnapshot",
    "prettier:components": "prettier --loglevel warn --write './src/uilib/{components,patterns}/**/*.{js,md}' './src/docs/uilib/{components,elements,patterns}/*.{js,md}'",
    "prettier:diff": "prettier --list-different '{scripts,stories,src}/**/*.{js,md}'",
    "prettier:js": "prettier --loglevel warn --write '{scripts,stories,src}/**/*.js'",
    "prettier:js:staged": "prettier --loglevel warn",
    "prettier:other": "prettier --loglevel warn --write 'src/**/*.{json,md}' --write '.babelrc' && yarn prettier:package",
    "prettier:other:staged": "prettier --loglevel warn",
    "prettier:package": "prettier-package-json --write ./package.json"
  },
  "dependencies": {
    "algoliasearch": "^4.8.0",
    "css-vars-ponyfill": "^2.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@emotion/babel-plugin": "^11.0.0",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@mdx-js/mdx": "^1.6.21",
    "@mdx-js/react": "^1.6.21",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "camelcase": "^6.2.0",
    "ci-info": "^2.0.0",
    "classnames": "^2.2.6",
    "clean-html": "^1.5.0",
    "cross-env": "^7.0.2",
    "current-git-branch": "^1.1.0",
    "date-fns": "^2.16.1",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.0.1",
    "gatsby": "^2.27.0",
    "gatsby-link": "^2.6.0",
    "gatsby-plugin-algolia": "^0.16.1",
    "gatsby-plugin-catch-links": "^2.5.0",
    "gatsby-plugin-emotion": "^4.5.0",
    "gatsby-plugin-manifest": "^2.7.0",
    "gatsby-plugin-mdx": "^1.5.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-offline": "^3.5.0",
    "gatsby-plugin-page-creator": "^2.5.0",
    "gatsby-plugin-postcss": "^3.2.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-sass": "^2.6.0",
    "gatsby-plugin-sharp": "^2.9.0",
    "gatsby-react-router-scroll": "^3.2.0",
    "gatsby-remark-images": "^3.6.0",
    "gatsby-source-filesystem": "^2.6.0",
    "gh-pages": "^3.1.0",
    "github-slugger": "^1.3.0",
    "jest": "^26.6.3",
    "jest-emotion": "^11.0.0",
    "jest-raw-loader": "^1.0.1",
    "keycode": "^2.2.0",
    "lint-staged": "^10.5.1",
    "nodemon": "^2.0.6",
    "ora": "^5.1.0",
    "postcss": "^8.1.8",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.0",
    "prettier-package-json": "^2.1.3",
    "pretty": "^2.0.0",
    "prism-react-renderer": "^1.1.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-fake-props": "^0.3.2",
    "react-live": "^2.2.3",
    "react-markdown": "^5.0.3",
    "start-server-and-test": "^1.11.6",
    "stylelint": "^13.8.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "buildVersion": "[LOCAL BUILD]",
  "volta": {
    "extends": "../../package.json"
  }
}
