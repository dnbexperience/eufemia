{
  "private": true,
  "name": "dnb-design-system-portal",
  "version": "1.0.0",
  "description": "DNB Design System Portal",
  "license": "SEE LICENSE IN LICENSE FILE",
  "author": "DNB Team & Tobias HÃ¸egh",
  "main": "src/docs/index.js",
  "scripts": {
    "build": "NODE_NO_WARNINGS=1 gatsby build",
    "build-ci": "yarn build:version && yarn build --prefix-paths",
    "build-visual-test": "SKIP_IMAGE_PROCESSING=1 IS_VISUAL_TEST=1 yarn build --no-uglify",
    "build:version": "node ./scripts/version.js --new-version",
    "clean": "NODE_NO_WARNINGS=1 gatsby clean",
    "precommit": "yarn lint-staged",
    "cy": "cypress open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --port 4000 --headless --spec './cypress/integration/**/*'",
    "cypress:run-ci": "start-server-and-test cypress:server http://localhost:3030 cypress:run",
    "cypress:server": "gatsby serve -p 3030",
    "lint": "eslint --quiet ./src",
    "lint-staged": "lint-staged",
    "lint:ci": "NODE_ENV=test yarn lint:js && yarn lint:styles",
    "lint:js": "yarn lint --fix",
    "lint:js:staged": "eslint --quiet --fix",
    "lint:styles": "stylelint './src/**/{Examples.*,*.scss}'",
    "lint:styles:staged": "stylelint './src/**/{Examples.*,*.scss}'",
    "reset": "yarn clean && rm -rf ./node_modules",
    "serve": "NODE_NO_WARNINGS=1 gatsby serve -p 8000",
    "start": "NODE_NO_WARNINGS=1 cross-env gatsby develop",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --passWithNoTests",
    "test:staged": "jest --bail --findRelatedTests ",
    "test:screenshots": "yarn workspace @dnb/eufemia test:screenshots",
    "test:screenshots:ci": "start-server-and-test serve http://localhost:8000 'yarn workspace @dnb/eufemia test:screenshots:ci'",
    "test:types": "tsc --noEmit",
    "test:types:watch": "tsc --noEmit --watch",
    "test:update": "jest --updateSnapshot"
  },
  "dependencies": {
    "@dnb/eufemia": "workspace:*",
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "algoliasearch": "4.12.0",
    "classnames": "2.3.1",
    "css-vars-ponyfill": "2.4.7",
    "history": "5.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-live-ssr": "workspace:*",
    "smoothscroll-polyfill": "0.4.4"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.18.2",
    "@babel/node": "7.16.0",
    "@emotion/cache": "11.9.3",
    "@typescript-eslint/parser": "5.43.0",
    "babel-jest": "29.3.1",
    "camelcase": "6.2.0",
    "cross-env": "7.0.3",
    "current-git-branch": "1.1.0",
    "date-fns": "2.25.0",
    "dotenv": "10.0.0",
    "eslint": "8.1.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "front-matter": "4.0.2",
    "fs-extra": "10.0.0",
    "gatsby": "4.24.4",
    "gatsby-link": "4.24.1",
    "gatsby-plugin-algolia": "0.26.0",
    "gatsby-plugin-babel-react-live": "1.0.1",
    "gatsby-plugin-catch-links": "4.24.0",
    "gatsby-plugin-emotion": "8.0.0",
    "gatsby-plugin-eufemia-theme-handler": "workspace:*",
    "gatsby-plugin-gatsby-cloud": "4.24.0",
    "gatsby-plugin-manifest": "4.24.0",
    "gatsby-plugin-mdx": "3.20.0",
    "gatsby-plugin-meta-redirect": "1.1.1",
    "gatsby-plugin-offline": "5.24.0",
    "gatsby-plugin-page-creator": "4.24.1",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sass": "5.24.0",
    "gatsby-plugin-scroll-position": "1.2.1",
    "gatsby-plugin-sharp": "4.24.0",
    "gatsby-react-router-scroll": "5.24.0",
    "gatsby-remark-images": "6.24.0",
    "gatsby-source-filesystem": "4.24.0",
    "github-slugger": "1.4.0",
    "is-absolute-url": "4.0.1",
    "jest": "29.3.1",
    "keycode": "2.2.0",
    "lint-staged": "11.2.6",
    "ora": "5.4.1",
    "postcss": "8.3.11",
    "postcss-preset-env": "6.7.0",
    "prism-react-renderer": "1.3.3",
    "process": "0.11.10",
    "prop-types": "15.7.2",
    "raw-loader": "4.0.2",
    "react-live": "3.1.1",
    "react-markdown": "8.0.3",
    "remark-gfm": "1.0.0",
    "repo-utils": "workspace:*",
    "sass": "1.43.4",
    "start-server-and-test": "1.14.0",
    "storybook-utils": "workspace:*",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.21.0",
    "svg-react-loader": "0.4.6",
    "typescript": "4.5.5"
  },
  "buildVersion": "[LOCAL BUILD]",
  "releaseVersion": "[LOCAL BUILD]",
  "changelogVersion": "[LOCAL BUILD]",
  "volta": {
    "extends": "../../package.json"
  }
}
