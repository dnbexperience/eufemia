    "simple-git": "2.47.0",
    "stream-buffers": "3.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.21.0",
    "svg2vectordrawable": "2.7.1",
    "svgo": "2.8.0",
