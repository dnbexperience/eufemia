{
  "name": "@dnb/eufemia",
  "version": "0.0.0-development",
  "description": "DNB Eufemia Design System UI Library",
  "license": "SEE LICENSE IN LICENSE FILE",
  "author": "DNB Team & Tobias HÃ¸egh",
  "homepage": "https://eufemia.dnb.no/uilib",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnbexperience/eufemia.git"
  },
  "bugs": {
    "url": "https://github.com/dnbexperience/eufemia/issues"
  },
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "audit:ci": "yarn audit:ci:yarn",
    "audit:ci:npm": "audit-ci --config ./audit-ci.json --report-type summary",
    "audit:ci:yarn": "audit-ci --config ./audit-ci.json --package-manager=yarn --report-type summary",
    "audit:ci:yarn-outdated": "babel-node ./scripts/prepub/audit/toOpt && audit-ci --config ./audit-ci.json --package-manager=yarn --report-type full && babel-node ./scripts/prepub/audit/toDev",
    "build": "yarn build:clean && yarn build:prebuild && yarn build:esm && yarn build:copy",
    "prebuild:ci": "yarn build",
    "postbuild:ci": "yarn build:types && yarn build:cjs && yarn build:es && yarn build:umd && yarn build:esm && yarn build:copy && yarn prettier:other && echo \"Can be enabled in future -> yarn build:resources\"",
    "prebuild:ci:figma": "yarn figma",
    "postbuild:ci:figma": "yarn test:update && yarn test:screenshots icon -u && babel-node ./scripts/figma/tasks/commitChanges",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel ./src --extensions .cjs,.js --keep-file-extension --config-file ./babel.config.js --out-dir ./build/cjs --copy-files --no-copy-ignored --ignore 'src/esm,src/umd,src/core,**/*.test.js,**/__tests__/**/*'",
    "build:clean": "rm -rf build/**",
    "build:component": "cross-env NODE_ENV=production BABEL_ENV=esm babel ./src/components/global-status --extensions .js --config-file ./babel.config.js --out-dir ./build/esm/components/global-status",
    "build:copy": "./scripts/release/copy-build-artifacts.sh",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src --extensions .js --config-file ./babel.config.js --out-dir ./build/es --copy-files --no-copy-ignored --ignore 'src/cjs,src/esm,src/umd,src/core,**/*.test.js,**/__tests__/**/*'",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel ./src --extensions .js --config-file ./babel.config.js --out-dir ./build/esm --copy-files --no-copy-ignored --ignore 'src/cjs,src/esm,src/umd,src/core,**/*.test.js,**/__tests__/**/*'",
    "build:pack": "yarn build && yarn publish:prepare && cd ./build && yarn pack",
    "build:prebuild": "babel-node ./scripts/prepub/runPrepub.js",
    "build:resources": "babel-node ./scripts/prepub/resources/makeResourcesPackage.js",
    "build:shared": "cross-env NODE_ENV=production BABEL_ENV=esm babel ./src/shared --extensions .js --config-file ./babel.config.js --out-dir ./build/esm/shared --ignore '**/__tests__/**/*'",
    "build:types": "babel-node ./scripts/prepub/generateTypes.js && TEST_POST_TYPES=1 yarn jest ./postTypeGeneration.test.js",
    "build:types:dev": "nodemon --exec 'babel-node ./scripts/prepub/generateTypes.js' --ext js --watch './src/**/*' --watch './scripts/**/*'",
    "build:umd": "cross-env NODE_ENV=production BABEL_ENV=umd rollup -c ./rollup.config.js",
    "cdn": "live-server --no-browser --cors --port=8081",
    "precommit": "yarn lint-staged",
    "dev:cjs": "nodemon --exec 'yarn build:cjs && yarn build:copy' --ext js,html,json,css,scss --watch './src/**/*' --ignore './**/*'",
    "dev:components": "nodemon --exec 'yarn build:component && yarn build:shared && yarn build:copy' --ext js,html,json,css,scss --watch './src/**/*' --ignore '*.json'",
    "dev:icons": "nodemon --exec 'babel-node ./scripts/tools/convertIcons' --ignore '/icons/**' --ignore '*.json'",
    "dev:resources": "nodemon --exec 'babel-node ./scripts/prepub/resources/makeResourcesPackage.js' --ext js,html,json,css,scss --watch './build/style/**/*' --watch './scripts/**/*' --ignore '*.json'",
    "dev:tasks": "nodemon --exec 'babel-node ./scripts/prepub/dev.js' --watch 'rollup.config.js' --ext js,html,json,css,scss --watch './src/components/**/*' --watch './src/style/**/*' --watch './scripts/**/*' --ignore '*.json'",
    "dev:umd": "nodemon --exec 'yarn build:umd && yarn build:copy' --ext js,html,json,css,scss --watch './src/**/*' --ignore './umd/*'",
    "figma": "babel-node ./scripts/figma/updateAll",
    "figma:dev:commit": "nodemon --exec 'babel-node ./scripts/figma/tasks/commitChanges'",
    "figma:dev:icons": "nodemon --exec 'babel-node ./scripts/figma/updateIcons' --ignore '*.svg' --ignore '*.json'",
    "figma:refetch": "yarn figma --force-refetch",
    "figma:reset": "babel-node ./scripts/figma/resetIcons && babel-node ./scripts/tools/convertIcons",
    "jest": "cross-env NODE_ICU_DATA=../../node_modules/full-icu jest",
    "lint": "eslint --quiet ./src ./scripts",
    "lint-staged": "lint-staged -d",
    "lint:ci": "yarn lint:lockfile && yarn lint:js && yarn lint:styles",
    "lint:js": "yarn lint --fix",
    "lint:js:staged": "eslint --quiet --fix",
    "lint:lockfile": "yarn lockfile-lint --path ../../yarn.lock --type yarn --validate-https --allowed-hosts yarn github.com codeload.github.com",
    "lint:styles": "stylelint './src/**/*.{js,scss}'",
    "lint:styles:staged": "stylelint './{src,scripts}/**/*.{js,scss}'",
    "make-properties": "babel-node ./scripts/prepub/makeProperties",
    "publish:ci": "yarn publish:prepare && cd ./build && dotenv semantic-release",
    "publish:dry": "dotenv semantic-release --no-ci --dry-run",
    "publish:prepare": "babel-node ./scripts/release/prepareForRelease.js",
    "reset": "rm -rf ./node_modules ./components ./elements ./es ./icons ./extensions ./shared ./style ./umd",
    "skeleton:font": "nodemon --exec 'babel-node ./scripts/tools/createSkeletonFont.js'",
    "start": "yarn workspace dnb-eufemia-sandbox start",
    "test": "yarn jest",
    "test:ci": "yarn jest --ci",
    "test:screenshots": "yarn jest --config=./jest.config.screenshots.js --forceExit --detectOpenHandles",
    "test:screenshots:build": "yarn build && yarn workspace dnb-design-system-portal build-portal && yarn test:screenshots",
    "test:screenshots:ci": "yarn jest --config=./jest.config.screenshots.js --ci --forceExit --detectOpenHandles",
    "test:screenshots:recreate": "find . -name '*.snap.png' -type f|xargs rm -f && yarn test:screenshots",
    "test:screenshots:update": "yarn jest --config=./jest.config.screenshots.js --updateSnapshot --forceExit --detectOpenHandles",
    "test:screenshots:watch": "yarn jest --config=./jest.config.screenshots.js --forceExit --detectOpenHandles --watchAll --testPathPattern ",
    "test:staged": "yarn jest --bail --findRelatedTests ",
    "test:update": "yarn jest --updateSnapshot",
    "test:watch": "yarn jest --watchAll --testPathPattern ",
    "prettier:diff": "prettier --list-different '{scripts,src}/**/*.{md,js}'",
    "prettier:js": "prettier --loglevel warn --write '{scripts,src}/**/*.js'",
    "prettier:js:staged": "prettier --config ./.prettierrc --loglevel warn",
    "prettier:other": "prettier --loglevel warn --write './src/**/*.{json,md}' && yarn prettier:package",
    "prettier:other:staged": "prettier --loglevel warn",
    "prettier:package": "prettier-package-json --write ./package.json",
    "prettier:styles": "prettier --loglevel warn --parser scss --write './src/**/*.scss'",
    "prettier:styles:staged": "prettier --loglevel warn --parser scss"
  },
  "sideEffects": [
    "*.scss",
    "umd/*",
    "style/**/*",
    "es/style/**/*",
    "esm/style/**/*"
  ],
  "typings": "./index.d.ts",
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "classnames": "^2.3.1",
    "core-js": "^3.12.1",
    "date-fns": "^2.21.3",
    "keycode": "^2.2.0",
    "react-text-mask": "^5.4.3",
    "what-input": "^5.2.10"
  },
  "peerDependencies": {
    "react": ">=16.12",
    "react-dom": ">=16.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/parser": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.2",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-typescript": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/plugin-transform-react-constant-elements": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/traverse": "^7.14.2",
    "@mdx-js/react": "1.6.22",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@svgr/core": "^5.5.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "audit-ci": "^4.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-optimize-clsx": "^2.6.1",
    "babel-plugin-search-and-replace": "^1.1.0",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browserslist": "^4.16.6",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.1",
    "ci-info": "^3.1.1",
    "color": "^3.1.3",
    "cross-env": "^7.0.3",
    "current-git-branch": "^1.1.0",
    "del": "^6.0.0",
    "detect-port": "^1.3.0",
    "dnb-design-system-portal": "*",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "figma-js": "^1.13.0",
    "files-exist": "^1.1.0",
    "fontmin": "^0.9.8",
    "fs-extra": "^10.0.0",
    "full-icu": "^1.3.4",
    "globby": "^11.0.3",
    "gulp": "^4.0.2",
    "gulp-clone": "^2.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-transform": "^3.0.5",
    "history": "^5.0.0",
    "is-ci": "^3.0.0",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "jest-matchmedia-mock": "^1.1.0",
    "jest-raw-loader": "^1.0.1",
    "jest-screenshot": "^0.3.2",
    "jest-tobetype": "^1.2.3",
    "lint-staged": "^11.0.0",
    "live-server": "^1.2.1",
    "lockfile-lint": "^4.6.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isequalwith": "^4.4.0",
    "markdown-tables-to-json": "^0.1.7",
    "mkdirp": "^1.0.4",
    "node-sass": "^5.0.0",
    "node-sass-once-importer": "^5.3.2",
    "nodemon": "^2.0.7",
    "opentype.js": "^1.3.3",
    "ora": "^5.4.0",
    "packpath": "^0.1.0",
    "postcss": "^8.3.0",
    "postcss-calc": "^8.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "prettier-package-json": "^2.6.0",
    "prop-types": "^15.7.2",
    "puppeteer": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fake-props": "^0.3.2",
    "react-to-typescript-definitions": "^3.0.1",
    "rollup": "^2.49.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass-variable-parser": "^1.2.2",
    "semantic-release": "17.4.3",
    "simple-commit-message": "^4.1.2",
    "simple-git": "^2.39.0",
    "stream-buffers": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "svg2vectordrawable": "^2.6.26",
    "svgo": "^2.3.0",
    "tar": "^6.1.0",
    "traverse": "^0.6.6",
    "wait-on": "^5.3.0"
  },
  "keywords": [
    "Accessibility",
    "DNB",
    "Design System",
    "Eufemia",
    "UX"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": "simple-commit-message",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "release",
      "release-*",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "ci": true,
    "debug": false,
    "dryRun": false,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# Changelog\n\nAll notable changes to @dnb/eufemia will be documented in this file. See\n[Conventional Commits](https://conventionalcommits.org) for commit guidelines."
        }
      ],
      [
        "@semantic-release/npm",
        {
          "tarballDir": "../dist"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": "../dist/*.tgz"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [],
          "message": "chore(release): ${nextRelease.version} [CI SKIP]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}
