    "detect-port": "1.3.0",
    "dnb-design-system-portal": "workspace:*",
    "dotenv": "10.0.0",
    "dotenv-cli": "4.1.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.2.0",
