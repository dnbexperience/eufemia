  "name": "dnb-ui-lib",
  "description": "DNB UI Library",
  "license": "SEE LICENSE IN LICENSE FILE",
  "author": "DNB Team & Tobias HÃ¸egh",
  "homepage": "https://eufemia.dnb.no/uilib",
  "repository": {
    "type": "git",
