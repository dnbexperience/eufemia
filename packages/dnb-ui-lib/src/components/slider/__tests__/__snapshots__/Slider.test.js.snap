    vertical-align: inherit;
    /* 2 */ }

/*
* Slider component
*
*/
:root {
  --slider-line-height: 0.25rem;
  --slider-thumb-size: 2rem;
  --slider-button-size: 1.5rem;
  --slider-space: 1rem; }

.dnb-slider {
  display: flex;
  align-items: center; }
  .dnb-slider--vertical {
    flex: none;
    align-items: flex-start;
    min-width: inherit;
    min-height: inherit; }
  .dnb-slider__inner {
    display: inline-flex;
    height: var(--slider-thumb-size);
    width: auto; }
    .dnb-slider__inner:not(.dnb-slider__inner--vertical) {
      flex: 1; }
  .dnb-slider__track {
    position: relative;
    flex: 1;
    margin: 0 calc(var(--slider-space) * 1.5);
    cursor: pointer; }
    .dnb-slider__track::before, .dnb-slider__track::after {
      content: '';
      position: absolute;
      z-index: 2;
      left: 0;
      top: 50%;
      width: var(--slider-thumb-size);
      height: var(--slider-line-height);
      transform: translate3d(0, -50%, 0);
      border-radius: calc(var(--slider-line-height) / 2);
      background-color: inherit; }
    .dnb-slider__track::before {
      background-color: red;
      left: calc(1px - 1px - var(--slider-space));
      right: auto; }
    .dnb-slider__track::after {
      background-color: blue;
      left: auto;
      right: calc(1px - 1px - var(--slider-space)); }
  .dnb-slider--vertical .dnb-slider__track {
    margin: calc(var(--slider-space) * 1.5) 0; }
    .dnb-slider--vertical .dnb-slider__track::before, .dnb-slider--vertical .dnb-slider__track::after {
      left: 50%;
      transform: translate3d(-50%, 0, 0);
      width: var(--slider-line-height);
      height: var(--slider-thumb-size); }
    .dnb-slider--vertical .dnb-slider__track::before {
      top: calc(1px - 1px - var(--slider-space));
      bottom: auto; }
    .dnb-slider--vertical .dnb-slider__track::after {
      top: auto;
      bottom: calc(1px - 1px - var(--slider-space)); }
  .dnb-slider--vertical .dnb-slider__inner {
    flex-direction: column;
    width: var(--slider-thumb-size);
    height: inherit;
    min-width: inherit;
    min-height: inherit; }
  .dnb-slider--reverse .dnb-slider__track {
    transform: scaleX(-1); }
  .dnb-slider--reverse.dnb-slider--vertical .dnb-slider__track {
    transform: scaleY(-1); }
  .dnb-slider__line {
    position: absolute;
    left: 0;
    top: 50%;
    height: var(--slider-line-height);
    transform: translate3d(0, -50%, 0);
    background-color: grey; }
  .dnb-slider--vertical .dnb-slider__line {
    width: var(--slider-line-height);
    height: auto;
    left: 50%;
    top: 0;
    transform: translate3d(-50%, 0, 0); }
  .dnb-slider__line__before {
    z-index: 2; }
  .dnb-slider__line__after {
    z-index: 1;
    right: 0;
    background-color: #ddd; }
  .dnb-slider--vertical .dnb-slider__line__before {
    top: 0; }
  .dnb-slider--vertical .dnb-slider__line__after {
    bottom: 0; }
  .dnb-slider__thumb {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 3;
    display: block;
    transform: translate3d(-50%, 0, 0);
    border-radius: 50%; }
    .dnb-slider__thumb,
    .dnb-slider__thumb .dnb-button {
      height: var(--slider-thumb-size);
      width: var(--slider-thumb-size); }
  .dnb-slider--vertical .dnb-slider__thumb {
    transform: translate3d(0, -50%, 0); }
  .dnb-slider__button {
    transform: translateY(0.25rem);
    height: var(--slider-button-size);
    width: var(--slider-button-size); }
  .dnb-slider--vertical .dnb-slider__button {
    transform: translateX(0.25rem); }
  .dnb-slider__state--jumped .dnb-slider__thumb,
  .dnb-slider__state--jumped .dnb-slider__line {
    transition: width 250ms ease 0ms, height 250ms ease 0ms, left 250ms ease 0ms, top 250ms ease 0ms, box-shadow 250ms ease 0ms; }
  .dnb-slider__state--disabled.dnb-slider__track,
  .dnb-slider__state--disabled .dnb-slider__thumb {
    cursor: not-allowed; }
  .dnb-slider > .dnb-form-label {
    margin-right: 1rem; }
  .dnb-slider__label--vertical {
    flex-direction: column; }
  .dnb-slider__label--vertical.dnb-slider--vertical {
    align-items: center; }
    .dnb-slider__label--vertical.dnb-slider--vertical .dnb-form-label {
      text-align: center;
      width: 100%;
      margin-right: 0;
      margin-bottom: 1rem; }
  .dnb-slider > .dnb-form-status {
    margin-top: 0.5rem; }
"
